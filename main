#Project Euler #1


c=0
i=999
totalthree=0

k = 995
e = 0
totalfive=0

total = 0

while i > 0:
    c = totalthree + i
    print(totalthree,"+",i,"=")
    i = i-3
    totalthree = c
    #print((i//3),":",totalthree)
    print(c)

#print(totalthree)

while k > 0:
    e = totalfive + k
    k = k-5
    totalfive = e
    #print(totalfive)

print(totalfive)
total = totalthree + totalfive
print("total: ",total)
